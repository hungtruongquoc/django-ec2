option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: backend.wsgi:application

files:
  "/etc/nginx/conf.d/elasticbeanstalk/01_static.conf":
    mode: "000644"
    owner: "root"
    group: "root"
    content: |
      location /static {
          alias /var/app/current/static;
      }

      location /media {
          alias /var/app/current/media;
      }